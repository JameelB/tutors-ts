{% from "headelements/walllink.njk" import walllink %}

{% macro walllinks(lo) %}
  <div class="ui equal width grid">
    {{ walllink("talkwall.html", lo.talks) }}
    {{ walllink("labwall.html", lo.labs) }}
    {{ walllink("videowall.html", lo.videos) }}
    {{ walllink("repowall.html", lo.repos) }}
    {{ walllink("archivewall.html", lo.archives) }}
  </div>
{% endmacro %}