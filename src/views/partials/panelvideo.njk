{% macro panelvideo (lo) %}
  <div class="ui segment container">
    <h3 class="ui top attached header">
      <a href="https://youtu.be/{{ lo.videoid }}"  target="_blank"> <i class="video icon"></i>{{ lo.title }} </a>
    </h3>
    <div class="ui attached segment ">
      <div class="ui embed" data-source="youtube" data-id="{{ lo.videoid }}"></div>
    </div>
  </div>
{% endmacro %}